    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webdriverio": "^4.13.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@wdio/cli": "^9.18.1",
    "autoprefixer": "^10.4.21",
